{
	"name": "thegeneration/financerecruitment-millnet",
	"description": "Plugin boilerplate for The Generation's plugins",
	"type": "wordpress-plugin",
	"license": "proprietary",
	"authors": [
		{
			"name": "The Generation",
			"email": "info@thegeneration.se"
		}
	],
	"autoload": {
		"psr-4": {
			"Financerecruitment_Millnet\\": "inc"
		},
		"files": [
			"inc/helpers.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^7.5",
		"squizlabs/php_codesniffer": "3.*",
		"wp-coding-standards/wpcs":"^2.1.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"platform": {
			"php": "7.4"
		}
	}
}
